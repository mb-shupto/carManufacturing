<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="525.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="supto_1930875.TestCarController">
    <children>
        <!-- Title -->
        <Label layoutX="246.0" layoutY="38.0" text="Test Finished Cars">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>

        <!-- Car selection -->
        <Label layoutX="142.0" layoutY="155.0" text="Select Car:" />
        <ComboBox fx:id="carComboBox" layoutX="245.0" layoutY="151.0" prefHeight="26.0" prefWidth="273.0" promptText="Choose a Car" />

        <!-- Test result selection -->
        <Label layoutX="140.0" layoutY="222.0" text="Test Result:" />
        <ComboBox fx:id="resultComboBox" layoutX="245.0" layoutY="218.0" prefHeight="26.0" prefWidth="274.0" promptText="Choose a result" />

        <!-- Confirm Test button -->
        <Button layoutX="245.0" layoutY="295.0" onAction="#confirmTest" text="Confirm Test" />

        <!-- Back button -->
        <Button layoutX="570.0" layoutY="460.0" onAction="#backOnAction" text="Back" />
      <Label fx:id="outputLabel" layoutX="413.0" layoutY="93.0" prefHeight="18.0" prefWidth="238.0" />
    </children>
</AnchorPane>
